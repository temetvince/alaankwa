<!--  _____________________________________________________________________________________________ -->
<!--  Shield Expertise -->
<!--  _____________________________________________________________________________________________ -->
<!--  The maximum shield percentage of the target while using the Blueprint Data Scanner. -->
<!--  Set the Shield Expertise. Default: 7 -->
      <t id="134">7</t>
